    "markdown-it": "^12.0.6",
    "normalize.css": "^8.0.1",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.2.2",
    "react-icons": "^4.2.0",
    "react-markdown": "^6.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
