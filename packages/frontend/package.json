{
  "name": "@broccoli/frontend",
  "version": "3.6.2",
  "private": true,
  "scripts": {
    "start:dev": "PORT=9900 react-app-rewired start",
    "build": "INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "test": "CI=true react-app-rewired test --passWithNoTests",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l 3061",
    "start": "npm run build && node server.js",
    "lint": "tsc && eslint --no-error-on-unmatched-pattern ./src/**/* --ext .js,.ts,.tsx,.jsx",
    "lint:fix": "npm run lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.1%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      ">0.1%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ]
  },
  "dependencies": {
    "@broccoli/shared": "3.6.2",
    "@chakra-ui/icons": "^1.0.12",
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "chakra-ui-markdown-renderer": "^1.1.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.2",
    "fastify-static": "^3.3.0",
    "fp-ts": "^2.9.0",
