    "customize-cra": "^1.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
